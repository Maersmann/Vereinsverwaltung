<Page x:Class="Vereinsverwaltung.UI.Desktop.Schnurschiessen.Pages.SchnurschiessenOptionPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Vereinsverwaltung.UI.Desktop.Schnurschiessen.Pages"
      mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="800" 
        Background="{DynamicResource MaterialDesignPaper}">
    <Grid>

        <Grid.RowDefinitions>
            <RowDefinition Height="*" MinHeight="400px"/>
            <RowDefinition Height="5px"/>
            <RowDefinition Height="*" MinHeight="400px"/>
        </Grid.RowDefinitions>

        <Grid Grid.Row="0">
            <Frame x:Name="ContainerUp"  NavigationUIVisibility="Hidden"  />
        </Grid>

        <GridSplitter Grid.Row="1" Height="5" HorizontalAlignment="Stretch"/>

        <Grid Grid.Row="2">
            <Frame x:Name="ContainerDown"  NavigationUIVisibility="Hidden"  />
        </Grid>

    </Grid>
</Page>
