<Page x:Class="Vereinsverwaltung.UI.Desktop.Schluesselverwaltung.Pages.SchluesselverteilungSchluesselUebersichtPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Vereinsverwaltung.UI.Desktop.Schluesselverwaltung.Pages"
      mc:Ignorable="d" 
        d:DesignHeight="450" d:DesignWidth="800"
        DataContext=""
        Background="{DynamicResource MaterialDesignPaper}">

    <Grid>

        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" MinWidth="600px"/>
            <ColumnDefinition Width="5px"/>
            <ColumnDefinition Width="*" MinWidth="600px"/>
        </Grid.ColumnDefinitions>

        <Grid Grid.Column="0">
            <Frame x:Name="ContainerLeft"  NavigationUIVisibility="Hidden"  />
        </Grid>

        <GridSplitter Grid.Column="1" Width="5" HorizontalAlignment="Stretch"/>

        <Grid Grid.Column="2">
            <Frame x:Name="ContainerRight"  NavigationUIVisibility="Hidden" MinWidth="540"  />
        </Grid>

    </Grid>
</Page>
